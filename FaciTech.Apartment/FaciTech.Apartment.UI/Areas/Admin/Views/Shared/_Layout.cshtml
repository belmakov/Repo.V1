<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>FaciTech - @ViewData["Title"]</title>

    <!-- General CSS Files -->
    <link rel="stylesheet" href="@Url.Content("~/external/bootstrap/bootstrap.min.css")">
    <link href="@Url.Content("~/external/template/css/now-ui-dashboard.css")" rel="stylesheet" />
    <link href="@Url.Content("~/external/datatable/css/jquery.dataTables.css")" rel="stylesheet" />


    <link rel="stylesheet" href="/external/font-awesome/css/font-awesome.css" />
    <!-- CSS Libraries -->
    <!-- Template CSS -->

    <link rel="stylesheet" href="~/css/style.css">

    <style>
    </style>
    <script src="~/external/jquery/jquery-3.4.0.min.js"></script>
    <script src="@Url.Content("~/external/datatable/js/jquery.dataTables.min.js")"></script>
</head>
<body>

    <div class="wrapper ">
        @await Html.PartialAsync("_Sidebar")
        <div class="main-panel" id="main-panel">
            <!-- Navbar -->
            @await Html.PartialAsync("_NavigationBar")
            <!-- End Navbar -->
            @await Html.PartialAsync("_PanelHeader")
            @RenderBody()
        </div>
    </div>
    

    <div >
        Copyright &copy; FaciTech 2019
    </div>
    <!-- General JS Scripts -->

    <script src="@Url.Content("~/external/popper/popper.min.js")"></script>
    <script src="@Url.Content("~/external/bootstrap/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/external/template/js/plugins/bootstrap-notify.js")"></script>
    
    <!-- JS Libraies -->
    <!-- Template JS File -->
    <!-- Page Specific JS File -->

    @RenderSection("Scripts", required: false)
</body>
</html>
